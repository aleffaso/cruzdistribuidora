  <section id="products" class="portfolio">
    <div class="container" data-aos="fade-up">

      <div class="section-title">
        <h2>Produtos</h2>
        <p>Nossos produtos</p>
      </div>

      <div class="row" data-aos="fade-up" data-aos-delay="100">
        <div class="col-lg-12 d-flex justify-content-center">
          <ul id="portfolio-flters">
            <li data-filter="*" class="filter-active">Todos</li>
            <% products.forEach(products => {%>
              <li data-filter=".filter-<%= products.supplier.title %>"><%= products.supplier.title %></li>
            <% }) %>
          </ul>
        </div>
      </div>

      <div class="row portfolio-container" data-aos="fade-up" data-aos-delay="200">
        <% products.forEach(products => {%>
          <div class="col-lg-4 col-md-6 portfolio-item filter-<%= products.supplier.title %>">
            <img src="<%= products.picture %>" class="img-fluid-product" alt="">
            <div class="portfolio-info">
              <h4><%= products.title %></h4>
                <p>CÃ³digo: <%= products.code %></p>
            </div>
          </div>
        <% }) %>
      </div>

    </div>
  </section>